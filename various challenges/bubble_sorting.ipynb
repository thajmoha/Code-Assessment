   "source": [
    "# a=[6,4,1]\n",
    "a=[3,2,1]\n",
    "\n",
    "swap=0\n",
    "\n",
    "for i in range(len(a)):\n",
    "    for j in range(len(a)-1):\n",
    "        if a[j]>a[j+1]:\n",
    "            swap+=1\n",
    "            a[j],a[j+1] = a[j+1],a[j]\n",
    "print('Array is sorted in {0} swaps.'.format(swap)) \n",
    "print('First Element: ',a[0])\n",
    "print('Last Element: ',a[-1])"
   ]
